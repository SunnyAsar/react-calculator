{
  "env": {
      "browser": true,
      "es6": true,
      "jest/globals": true
  },
  "extends": "standard",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
      "space-before-function-paren": ["error", {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
      }]
  }
}